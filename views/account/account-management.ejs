<!-- Content Heading Section -->
<% if (title) { %>
    <h1 class="title_h1"><%= title %></h1>
    <% } else {
      res.redirect('/')
    } %>

     <!-- Show Flash Message -->
    <%- messages() %>
     <% if (errors) { %>
      <ul class="notice">
     <% errors.array().forEach(error => { %>
       <li><%= error.msg %></li>
    <%  }) %>
     </ul>
    <% } %>

    <div class = "account-mgn">
      <h1 id="account-name">Welcome <%= user.account_firstname %></h1>
      <p>You're logged in.</p>
    </div>